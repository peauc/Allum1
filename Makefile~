SRC=		src/main.c \
		src/reading.c \
		src/my_strlen.c \
		src/parsing.c \
		src/getinfo.c \
		src/my_putstr.c \

OBJ=		$(SRC:.c=.o)

NAME=		bsq

CC=		gcc

CFLAGS=		-W -Werror -Wall -Wextra -ansi -pedantic

$(NAME):	$(OBJ)
		gcc -o $(NAME) $(OBJ)

all:		$(NAME)

clean:
		rm $(OBJ)

fclean:		clean
		rm $(NAME)

re:		fclean all
