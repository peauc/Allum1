#include "allum1.h"

int	*create_array(t_allum *allum)
{
  int	i;

  i = -1;
  if ((allum->tab = malloc(5 * sizeof(int *))) == NULL)
    return (NULL);
  allum->tab[4] = -1;
  while (++i < 4)
    {
      allum->tab[i] = i * 2 + 1;
      printf("tab[%d] = %d\n", i, allum->tab[i]);
    }
  showallum(allum);
  allum->max = decal_max(allum);
  return (allum->tab);
}
